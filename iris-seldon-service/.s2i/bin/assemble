#!/bin/bash

echo "---> Showing env varibales for debug"
printenv

# run the default assemble script
echo "---> Running the default assemble script ..."
/usr/libexec/s2i/assemble

echo "---> Running custom asseble script ..."

echo "---> Build environment variables ..."
python model/config.py

echo "---> Downloading model file ..."
python model/loadmodel.py
